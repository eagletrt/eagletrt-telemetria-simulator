{
  "name": "@eagletrt/eagletrt-telemetria-simulator",
  "version": "3.2.2",
  "description": "EagleTRT telemetry simulator, in order to simulate on local machine the canbus and the gps serial port",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "bin": {
    "eagle": "./dist/bin/index.js"
  },
  "files": [
    "package.json",
    "package-lock.json",
    "dist",
    "default_sources",
    "gps_simulator",
    "settings",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "gps:compile": "cd gps_simulator && ./compile.sh",
    "transpile": "tsc -p source",
    "lint": "eslint source/**/*.ts --format codeframe",
    "lint:fix": "eslint --fix source/**/*.ts --format codeframe",
    "commit": "cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eagletrt/eagletrt-telemetria-simulator.git"
  },
  "author": "Eugenio Berretta <euberdeveloper@gmail.com> & Giacomo Ascari <giacomo.ascari@studenti.unitn.it>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eagletrt/eagletrt-telemetria-simulator/issues"
  },
  "homepage": "https://github.com/eagletrt/eagletrt-telemetria-simulator#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "module-alias": "^2.2.2",
    "shelljs": "^0.8.4",
    "tree-kill": "^1.2.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@euberdeveloper/eslint-config-typescript": "^4.4.0",
    "@types/node": "^14.14.14",
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^15.0.12",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@": "dist",
    "@lib": "dist/lib",
    "@bin": "dist/bin"
  }
}
